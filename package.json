{
  "name": "8ehradioitb",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=10",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --strict",
    "vercel-build": "bunx prisma generate && next build",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\" \"lib/**/*.{js,ts}\"",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx}\" \"lib/**/*.{js,ts}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "bun run build && playwright test",
    "test:e2e:smoke": "bun run build && playwright test --grep smoke",
    "prepare": "husky"
  },
  "dependencies": {
