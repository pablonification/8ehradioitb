    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "swiper": "^11.0.0",
    "swr": "^2.3.8",
    "wavesurfer.js": "^7.10.1"
