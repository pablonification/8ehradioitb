    "ai": "^5.0.106",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "formidable": "^3.5.4",
    "icy": "^2.1.0",
    "nanoid": "^5.1.5",
    "next": "^15.5.12",
